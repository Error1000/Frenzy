	{back}

OUTSIDE:
	TOA {cache_a}
	TOB {cache_b}
	

	FROMB {cache_b}
	TOB   {site1_addr}
	FROMB {back}
	TOB   {cache_b}
	JMP
	{ROTLA}		
site1:
	SUM


HLT: JMP
